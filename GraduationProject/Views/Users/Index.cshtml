 @model IEnumerable<User>

<h2>Users</h2>
<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Title</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>@user.Title</td>
                    <td>
                        <a href="@Url.Action("Details", "Users", new { id = user.Id })" class="btn btn-primary">Details</a>
                        <a href="@Url.Action("Edit", "Users", new { id = user.Id })" class="btn btn-warning">Edit</a>
                        <a href="@Url.Action("Delete", "Users", new { id = user.Id })" class="btn btn-danger">Delete</a>
                    </td>
                </tr>
        }
    </tbody>
</table>

<hr />